<rxui:ReactiveUserControl
    x:Class="richClosure.Avalonia.Views.PacketHexUserControl"
    x:TypeArguments="vms:PacketHexViewModel"
    xmlns:rxui="http://reactiveui.net"
    xmlns:vms="clr-namespace:richClosure.Avalonia.ViewModels"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    mc:Ignorable="d">
    <Grid>
        <TextBox x:Name="PacketHexTextBox" HorizontalAlignment="Left" Height="155" Margin="10,430,0,0"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="411"
                 FontFamily="Global Monospace"/>

        <TextBox x:Name="PacketAsciiTextBox" HorizontalAlignment="Left" Height="155" Margin="426,430,0,0"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="416"
                 FontFamily="Global Monospace"/>
    </Grid>
</rxui:ReactiveUserControl>